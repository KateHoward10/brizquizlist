<li class="quiz-entry">
  <span class="icon-container" style="background-color: hsl(<%= quiz.hue %>, 50%, 50%)"><%= fa_icon "home", class: "fa-lg home" %></span>
  <div class="quiz-overview">
    <span><%= quiz.venue %></span>
    <span class="quiz-time"><%= quiz.day.slice(0,3) %><%= quiz.time ? ", #{quiz.time}" : "" %></span>
  </div>
</li>
