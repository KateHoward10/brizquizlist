<li class="quiz-entry">
  <span class="icon-container" style="background-color: hsl(<%= 255 / @quizzes.length * quiz.id %>, 50%, 50%)"><i class="fas fa-home fa-lg home"></i></span>
  <div class="quiz-details">
    <span><%= quiz.venue %></span>
    <span class="quiz-time"><%= quiz.day.slice(0,3) %><%= quiz.time ? ", #{quiz.time}" : "" %></span>
  </div>
</li>
